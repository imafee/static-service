{
  "private": true,
  "name": "static-service",
  "version": "0.0.0",
  "description": "a simple zero-configuration command-line http server,can be used for command line or middleware",
  "author": "imafee <imafee@163.com> (https://)",
  "keywords": [
    "http",
    "https",
    "static",
    "svr",
    "serve",
    "server",
    "service",
    "middleware",
    "library",
    "command line",
    "cli"
  ],
  "license": "MIT",
  "homepage": "https://",
  "bugs": {
    "url": "https://github.com/imafee/static-service/issues",
    "email": "imafee@163.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:imafee/static-service.git"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://"
    }
  ],
  "type": "module",
  "bin": {
    "static-service": "bin/index.cjs"
  },
  "main": "dist/bundle.min.mjs",
  "exports": {
    ".": "./dist/bundle.min.mjs",
    "./cjs": "./dist/bundle.min.cjs"
  },
  "files": [
    "bin/**/*",
    "dist/**/*"
  ],
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "engines": {
    "node": ">=14 <=20"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "sideEffects": true,
  "scripts": {
    "start": "node index.js",
    "build": "npm-run-all eslint:check clean --parallel build:*",
    "release": "standard-version",
    "commit": "git-cz",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "---------splitLine---------": "",
    "prettier:help": "prettier --help",
    "prettier:check": "prettier --cache --check .",
    "prettier:write": "prettier --cache --write .",
    "eslint:help": "eslint --help",
    "eslint:check": "eslint --cache **/*.{js,cjs}",
    "eslint:write": "eslint --cache --fix **/*.{js,cjs}",
    "commitlint:check": "commitlint --edit ${1}",
    "build:mainesm": "rollup -c config/rollup/rollup.config.main.esm.cjs",
    "build:maincjs": "rollup -c config/rollup/rollup.config.main.cjs",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime-corejs3": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.3",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "inquirer": "^8.2.5"
  }
}
